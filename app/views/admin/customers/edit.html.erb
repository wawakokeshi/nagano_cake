<h2>???さんの会員情報編集</h2>

<%=　form_with model : @customers, url: edit_admin_customer_path, local:true do |f| %>
 <%= f.text_field :last_name, "氏名" %>
 <%= f.text_field :first_name %>
 <%= f.text_field :postal_code, "郵便番号" %>
 <%= f.text_field :address, "住所" %>
 <%= f.text_field :telephone_number, "電話番号" %>
 <%= f.text_field :email, "メールアドレス" %>
 <%#= f.text_field :is_active, "会員ステータス" %>
 <%= f.submit "変更を保存"%>
<% end %>